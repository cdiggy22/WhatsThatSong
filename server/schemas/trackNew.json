{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "new Track Schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "track_id": 1,
            "name": "Get Down On it",
            "artist_id": 2,
            "username": "user1"
        }
    ],
    "required": [
        "track_id",
        "name",
        "artist_id",
        "username"
    ],
    "properties": {
        "track_id": {
            "$id": "#/properties/track_id",
            "type": "integer",
            "title": "The track_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Get Down On it"
            ]
        },
        "artist_id": {
            "$id": "#/properties/artist_id",
            "type": "integer",
            "title": "The artist_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                2
            ]
        },
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "user1"
            ]
        }
    },
    "additionalProperties": true
}